{
  "name": "cucumber-playwright",
  "version": "1.0.0",
  "main": "index.js",
  "author": "David Tran",
  "license": "ISC",
  "description": "Cucumber Playwright Automation Framework",
  "scripts": {
    "prepReport": "rimraf reports traces && mkdir reports",
    "cucumber": "cucumber-js --config cucumber.mjs --format cucumber-console-formatter --require 'src/step-definitions/**/*.ts'",
    "build": "npx tsc",
    "gherkin-lint": "gherkin-lint -c .gherkin-lnt.yml",
    "exlint": "npx eslint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "engines": {
    "node": "20.10.0"
  },
  "dependencies": {
    "@cucumber/cucumber": "11.0.1",
    "@cucumber/html-formatter": "21.7.0",
    "@cucumber/messages": "26.0.1",
    "@cucumber/pretty-formatter": "1.0.1",
    "@playwright/test": "1.48.1",
    "@tensorflow/tfjs": "4.22.0",
    "@tensorflow/tfjs-node": "4.22.0",
    "cucumber-console-formatter": "1.0.0",
    "cucumber-html-formatter": "3.1.0",
    "cucumber-pretty": "6.0.1",
    "playwright": "1.48.1"
  },
  "devDependencies": {
    "@faker-js/faker": "9.0.3",
    "@types/fs-extra": "11.0.4",
    "@types/lodash": "4.17.12",
    "@types/node": "22.7.7",
    "@types/prettier": "3.0.0",
    "@typescript-eslint/eslint-plugin": "8.10.0",
    "@typescript-eslint/parser": "8.10.0",
    "allure-commandline": "2.30.0",
    "allure-cucumberjs": "^3.0.5",
    "allure-playwright": "^3.0.5",
    "as-a": "2.2.7",
    "axe-html-reporter": "2.2.11",
    "axe-playwright": "2.0.3",
    "axios": "1.7.7",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "fs-extra": "11.2.0",
    "gherkin-lint": "4.2.4",
    "husky": "9.1.6",
    "is-ci": "3.0.1",
    "lint-staged": "15.2.10",
    "open": "10.1.0",
    "prettier": "3.3.3",
    "read-excel-file": "5.8.6",
    "rimraf": "6.0.1",
    "standard-version": "9.5.0",
    "ts-node": "10.9.2",
    "tsc-files": "1.1.4",
    "typescript": "5.6.3"
  }
}
